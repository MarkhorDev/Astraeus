config_version=4

[application]

config/name="Astraeus"
config/description="An asteroids style low-res space shooter"
run/main_scene="res://Assets/Scenes/Levels/Menu.tscn"
boot_splash/show_image=false
config/icon="res://Assets/Icons/icon.png"
config/macos_native_icon="res://Assets/Icons/icon.icns"
config/windows_native_icon="res://Assets/Icons/icon.ico"

[autoload]

SceneTransition="*res://Assets/Scenes/Other/SceneTransition.tscn"
SceneChanger="*res://Assets/Scripts/Autoload/SceneChanger.gd"
PlayerSettings="*res://Assets/Scripts/Autoload/PlayerSettings.gd"
AudioPlayer="*res://Assets/Scripts/Autoload/AudioPlayer.gd"

[display]

window/size/width=576
window/size/height=324
window/size/fullscreen=true
window/size/test_width=1152
window/size/test_height=648
window/stretch/mode="viewport"
window/stretch/aspect="keep"

